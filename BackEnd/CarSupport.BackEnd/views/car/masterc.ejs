<div id="grid"></div>
<script id="template" type="text/x-kendo-template">
    <div>
        <span>Identificación:</span>
        <input type="text" name="id" data-bind="value:Id" class="k-textbox" />
    </div>
    <div>
        <span>Placa:</span>
        <input type="text" name="licensePlate" data-bind="value:LicensePlate" class="k-textbox" />
    </div>
    <div>
        <span>Año:</span>
        <input type="text" name="year" data-bind="value:Year" class="k-textbox" />
    </div>
    <div>
        <span>Kilometraje actual:</span>
        <input type="text" name="currentMileage" data-bind="value:CurrentMileage" class="k-textbox" />
    </div>
    <div>
        <span>Tipo de combustible:</span>
        <input type="text" name="fuelType" data-bind="value:FuelType" class="k-textbox" />
    </div>
    <div>
        <span>Clase:</span>
        <input type="text" name="Class" data-bind="value:Class" class="k-textbox" />
    </div>
    <div>
        <span>Modelo:</span>
        <input type="text" name="model" data-bind="value:Model" class="k-textbox" />
    </div>
    <div>
        <span>Usuario:</span>
        <input type="text" name="user" data-bind="value:User" class="k-textbox" />
    </div>
</script>

