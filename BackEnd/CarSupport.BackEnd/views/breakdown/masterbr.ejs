<div id="grid"></div>
<script id="template" type="text/x-kendo-template">
    <div>
        <span>Descripción:</span>
        <textarea name="description" data-bind="value:Description" class="k-textbox" required/>
    </div>
    <div>
        <span>Causas:</span>
        <textarea name="causes" data-bind="value:Causes" class="k-textbox" required />
    </div>
    <div>
        <span>Consecuencias:</span>
        <textarea type="text" name="consequences" data-bind="value:Consequences" class="k-textbox" required />
    </div>
    <div>
        <span>Soluciones</span>
        <textarea name="solutions" data-bind="value:Solutions" class="k-textbox" required />
    </div>
    <div>
        <span>Clasificación:</span>
        <input type="text" name="rating" id="rating" required />
    </div>
    <div>
        <span>Parte</span>
        <input type="text" name="part" id="part" required />
    </div>

</script>

<script src="/scripts_business/manage_breakdown.js"></script>
<script>
    $(document).ready(function() {
        manageBreakdown.fn.init();
    });
    
</script>
