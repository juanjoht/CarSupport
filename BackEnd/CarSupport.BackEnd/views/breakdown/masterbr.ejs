<div id="grid"></div>
<script id="template" type="text/x-kendo-template">
    <div>
        <span>Descripción:</span>
        <input type="text" name="description" data-bind="value:Description" class="k-textbox" />
    </div>
    <div>
        <span>Causas:</span>
        <input type="text" name="causes" data-bind="value:Causes" class="k-textbox" />
    </div>
    <div>
        <span>Consecuencias:</span>
        <input type="text" name="consequences" data-bind="value:Consequences" class="k-textbox" />
    </div>
    <div>
        <span>Soluciones</span>
        <input type="text" name="solutions" data-bind="value:Solutions" class="k-textbox" />
    </div>
    <div>
        <span>Clasificación:</span>
        <input type="text" name="rating" data-bind="value:Rating" class="k-textbox" />
    </div>
    <div>
        <span>Parte</span>
        <input type="text" name="part" data-bind="value:Part" class="k-textbox" />
    </div>
</script>

